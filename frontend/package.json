{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.3.3",
    "bootstrap": "^4.6.2",
    "clsx": "^1.2.1",
    "interweave": "^13.0.0",
    "ky": "^0.31.3",
    "path-to-regexp": "^6.2.1",
    "purecss": "^2.1.0",
    "qrcode.react": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-popper": "^2.3.0",
    "react-router-dom": "^5.3.3",
    "react-scripts": "3.4.4",
    "restful-react": "^15.9.4",
    "swiper": "^6.8.4",
    "timeago-react": "^3.0.5",
    "timeago.js": "^4.0.2",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "NODE_ENV=production INLINE_RUNTIME_CHUNK=false IMAGE_INLINE_SIZE_LIMIT=0 react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && prettier --check src/**/*.ts{,x} && eslint src/**/*.ts{,x}",
    "format-and-lint": "npm run format && npm run lint",
    "generate-client": "restful-react import --url http://localhost:8000/openapi.json --output src/generated-api-client.tsx",
    "postgenerate-client": "prettier src/generated-api-client.tsx --write",
    "prepare-upgrade": "git checkout master && git pull origin master && git checkout -b frontend-deps-upgrade-$(date -Idate)",
    "upgrade-all": "npm ci && npm upgrade && REACT_APP_BACKEND_BASE_PATH=https://api.staging.muenster-jetzt.de npm run start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/history": "^4.7.11",
    "@types/lodash-es": "^4.17.6",
    "@types/qrcode.react": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "csp-html-webpack-plugin": "^5.1.0",
    "customize-cra": "^1.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-sass": "4.14.1",
    "prettier": "^2.7.1",
    "react-app-rewired": "^2.2.1"
  }
}
